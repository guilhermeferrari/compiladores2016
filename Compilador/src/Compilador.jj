void termo6(Expressao expressao) : { Token t=null; int cont=0; }
{
    (
        t = <NAO>
        {
            expressao.addCodigoInfixo(t.image);
            cont++;
        }
    )*
    termo7(expressao)
    {
        if(t!=null) {
          if((cont%2)==1) {
            //Adiciona 1 para Impar
                expressao.addItem(new Item(Tipo.OPERADOR, t.image));
                cont = 1;
            
          }
          else {
			//Adiciona 2 para pares
			cont = 2;
			while(cont > 0) {
			 	 expressao.addItem(new Item(Tipo.OPERADOR, t.image));
              	 cont--;
			  }
			
          }
           
        }
    }
