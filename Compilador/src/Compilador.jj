// 1a prova
// o comando select-option-endSelect sera transformado em apenas um comando condicional
// onde cada bloco de comandos da parte FALSO de uma condicional sera composto pelo
// proximo comando condicional que equivale ao proximo option
Comando comandoSelect() : { Token tVar;
  						    Comando comCondicional;
  						    ListaComandos listaCondicionais = new ListaComandos(); }{	<SELECT> <PARL> tVar = <NOMEVAR> <PARR>
	// pelo menos uma clausula option
	(	   comCondicional = comandoOption(tVar.image)
	   {
	      // alimenta a lista de condicionais: todos ainda de forma separada
	      listaCondicionais.insere(comCondicional);	   }
	)+
	<ENDSELECT>
	{       // faz o encadeamento das condicionais: a lista falso do atual eh o prox comando condicional
       for(int i=0; i<listaCondicionais.getComandos().size()-1; i++)       {          Comando comAtual = listaCondicionais.getComando(i);
          Comando comProx  = listaCondicionais.getComando(i+1);
          ListaComandos listaFalso = new ListaComandos();
          listaFalso.insere(comProx);
          ((Condicional)comAtual).setFalso(listaFalso);
	   }
	   // apos o encadeamento, retorna apenas o primeiro comando condicional da lista
	   return listaCondicionais.getComando(0);
	}
}

// 1a prova
// metodo que representa apenas um option
Comando comandoOption(String nomeVar) : { Token tVar, tSub, tNum;
			                              Expressao expOption = new Expressao();
                                          ListaComandos blocoComandos;
                                          Comando comCondicional = new Condicional();
  			                            }
{
	{
	  tSub = null;
	}	  
    <OPTION> <PARL> (<ADD> | tSub=<SUB>)? tNum = <NUMERO> 
    {
      // monta o posfixo para o primeiro numero: variavel valor operador
      expOption.addItem(new Item(Tipo.VARIAVEL, nomeVar));
      if(tSub != null)
         expOption.addItem(new Item(Tipo.NUMERO, "-"+tNum.image)); 
      else
         expOption.addItem(new Item(Tipo.NUMERO, tNum.image)); 
      expOption.addItem(new Item(Tipo.OPERADOR, "=="));
    }
	// os outros numeros da clausula option sao opcionais
	(
	    {
	       tSub = null;
	    }
		(<ADD> | tSub=<SUB>)? tNum = <NUMERO>
	    {
    		// monta o posfixo para os outros numeros: variavel valor operador OR
      		expOption.addItem(new Item(Tipo.VARIAVEL, nomeVar)); 
            if(tSub != null)
               expOption.addItem(new Item(Tipo.NUMERO, "-"+tNum.image)); 
            else
               expOption.addItem(new Item(Tipo.NUMERO, tNum.image)); 
      		expOption.addItem(new Item(Tipo.OPERADOR, "=="));
      		expOption.addItem(new Item(Tipo.OPERADOR, "ou"));
    	}
	)*
	<PARR>
    blocoComandos = listaComandos()
    {
	   ((Condicional)comCondicional).setExpressao(expOption);
	   ((Condicional)comCondicional).setVerdadeiro(blocoComandos);
	   ((Condicional)comCondicional).setFalso(null);	   
	   return comCondicional;
    }
}


